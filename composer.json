{
	"description" : "The CodeIgniter Application with Composer",
	"name": "roniwahyu/civuestarterku",
	"type": "project",
	"keywords": [
        "CodeIgniter",
        "Composer",
        "installer",
        "VueJS",
        "Starter"
    ],
	"homepage": "http://roniwahyu.com",
	"license": "MIT",
	 "authors": [
        {
            "name": "Syahroni Wahyu Iriananda",
            "email": "roniwahyu@gmail.com"
        }, {
            "name": "Kenji Suzuki",
            "email": "kenji.uui@gmail.com"
        }
    ],
     "autoload": {
        "psr-4": {
            "Kenjis\\CodeIgniter\\": "src/"
        }
    },
    "scripts" : {
        "post-create-project-cmd": [
            "Kenjis\\CodeIgniter\\Installer::postInstall"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
	"support": {
		"forum": "http://forum.codeigniter.com/",
		"wiki": "https://github.com/bcit-ci/CodeIgniter/wiki",
		"slack": "https://codeigniterchat.slack.com",
		"source": "https://github.com/bcit-ci/CodeIgniter"
	},
	"require": {
		"php": ">=5.3.2",
		"codeigniter/framework": "3.1.*"
	},
	"suggest": {
		"paragonie/random_compat": "Provides better randomness in PHP 5.x",
		"kenjis/ci-phpunit-test": "An easier way to use PHPUnit with CodeIgniter 3.0",
        "kenjis/codeigniter-ss-twig": "A Simple and Secure Twig integration for CodeIgniter 3.0",
        "kenjis/codeigniter-doctrine": "A simple Doctrine integration for CodeIgniter 3.0"
	},
	"require-dev": {
		"mikey179/vfsStream": "1.1.*",
		"phpunit/phpunit": "4.* || 5.*"
		
	}
	
}




